{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "konva@9.3.8",
      "destination": "wwwroot/konva/",
      "files": [
        "konva.js",
        "konva.min.js"
      ]
    },
    {
      "library": "axios@1.6.8",
      "destination": "wwwroot/axios/"
    },
    {
      "library": "@microsoft/signalr@6.0.6",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}